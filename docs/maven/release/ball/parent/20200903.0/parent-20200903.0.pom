            <dependency>
              <groupId>ball</groupId>
              <artifactId>assembly-descriptors</artifactId>
              <version>${ball-parent.version}</version>
            </dependency>
          </dependencies>
          <configuration>
            <tarLongFileMode>posix</tarLongFileMode>
          </configuration>
        </plugin>
