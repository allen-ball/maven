      </activation>
      <build>
        <plugins>
          <plugin>
            <groupId>ball</groupId>
            <artifactId>ball-license-maven-plugin</artifactId>
            <executions>
              <execution>
                <goals>
                  <goal>update-project-license</goal>
                  <goal>generate-license-resources</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
